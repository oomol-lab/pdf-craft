{
  "editor.tabSize": 2,
  "editor.insertSpaces": true,
  "files.trimTrailingWhitespace": true,
  "pylint.args": [
    "--generate-members"
  ],
  "files.associations": {
    "*.jinja": "jinja-xml",
    "*.xml": "xml"
  },
  "[jinja-xml]": {
    "editor.wordWrap": "on"
  },
  "[xml]": {
    "editor.wordWrap": "on"
  },
  "cSpell.words": [
    "analyser",
    "apos",
    "fitz",
    "frombytes",
    "kwoa",
    "langchain",
    "layoutreader",
    "natsorted",
    "pixmap",
    "rects",
    "tiktoken",
    "tobytes"
  ]
}